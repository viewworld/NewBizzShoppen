<%= content_for :column_mc do -%>
  <%= header do |section| %>
    <% section.block_header do %>
      <%= @survey.name %>
    <% end %>
  <% end %>

  <% if @survey.thank_you_page.blank? %>
    <%= blank_state_message t("surveys.show.view.thank_you_msg") %>
  <% else %>
    <%= @survey.thank_you_page.html_safe %>
  <% end %>
<% end %>
