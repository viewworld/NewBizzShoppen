<%= content_for :column_mc do -%>
  <%= header(:back => true) do |section| %>
    <% section.block_header do %>
      <%= t("agent_timesheets.new.scheduled_actions") %>
    <% end %>
    <% section.block_actions do %>
      <ul class="header_actions">
        <li class="last">
          <%= bt_link_to nil, t("users.delayed_jobs.clear_all"), delete_all_users_delayed_jobs_path %>
        </li>
      </ul>
    <% end %>
  <% end %>

  <%= render :partial => 'shared/delayed_jobs/user', :locals => {:user => current_user, :delayed_jobs => @delayed_jobs} %>
<% end %>
