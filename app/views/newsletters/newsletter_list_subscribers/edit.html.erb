<column-mc:>
  <header back="true"><%= t("newsletters.newsletter_list_subscribers.edit.view.header") %></header>
  <%= semantic_form_for @newsletter_list_subscriber, :url => newsletters_newsletter_list_newsletter_list_subscriber_path(@newsletter_list, @newsletter_list_subscriber) do |form| %>
      <frm>
        <param:content>
          <%= render :partial => 'form', :locals => {:form => form} %>
        </param:content>
        <param:actions>
          <ibt c="fltri" icon="save">
            <%= form.submit t("newsletters.newsletter_list_subscribers.edit.view.button_update") %>
          </ibt>
          <ibt c="bt_grey fltri">
            <%= button_to_function t("newsletters.newsletter_list_subscribers.edit.view.button_cancel"), "history.go(-1)" %>
          </ibt>
        </param:actions>
      </frm>
  <% end %>
</column-mc:>