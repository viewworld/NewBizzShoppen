<%= content_for :column_mc do -%>
  <%= header(:back => true) do |section| %>
    <% section.block_header do %>
      <%= t("newsletters.newsletter_list_subscribers.new.view.header") %>
    <% end %>
  <% end %>

  <%= semantic_form_for @newsletter_list_subscriber, :url => newsletters_newsletter_list_newsletter_list_subscribers_path do |form| %>
      <%= frm do |section| %>
        <% section.block_content do %>
          <%= render :partial => 'form', :locals => {:form => form} %>
        <% end %>
        <% section.block_actions do %>
          <%= ibt(:class => "fltri", :icon => "save") do %>
            <%= form.submit t("newsletters.newsletter_list_subscribers.new.view.button_create") %>
          <% end %>
          <%= ibt(:class => "bt_grey fltri") do %>
            <%= button_to_function t("newsletters.newsletter_list_subscribers.new.view.button_cancel"), "history.go(-1)" %>
          <% end %>
        <% end %>
      <% end %>
  <% end %>
<% end %>
