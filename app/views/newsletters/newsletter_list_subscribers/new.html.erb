<column-mc:>
  <header back="true"><%= t("newsletters.newsletter_list_subscribers.new.view.header") %></header>
  <%= semantic_form_for @newsletter_list_subscriber, :url => newsletters_newsletter_list_newsletter_list_subscribers_path do |form| %>
      <frm>
        <param:content>
          <%= render :partial => 'form', :locals => {:form => form} %>
        </param:content>
        <param:actions>
          <ibt c="fltri" icon="save">
            <%= form.submit t("newsletters.newsletter_list_subscribers.new.view.button_create") %>
          </ibt>
          <ibt c="bt_grey fltri">
            <%= button_to_function t("newsletters.newsletter_list_subscribers.new.view.button_cancel"), "history.go(-1)" %>
          </ibt>
        </param:actions>
      </frm>
  <% end %>
</column-mc:>