<%= content_for :column_mc do -%>
  <%= header(:back => true) do |section| %>
    <% section.block_header do %>
      <%= t("administration.results.edit.view.title") %>: <%= @result.name %>
    <% end %>
  <% end %>

  <%= semantic_form_for @result, :url => administration_result_path, :method => :post do |form| %>
      <frm title="Update existing">
        <param:content>
          <%= render :partial => '/callers/results/form', :locals => {:form => form, :result => @result} %>
        </param:content>
        <param:actions>
          <%= ibt(:class => "fltri", :icon => "save") do %>
            <%= form.submit t("administration.results.edit.view.button_update"), :name => "commit_create" %>
          <% end %>
        </param:actions>
      </frm>
  <% end %>
<% end %>
