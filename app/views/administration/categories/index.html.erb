<column-mc:>
<h1>Categories</h1>

  <%= link_to t("administration.categories.index.view.new_category"), new_administration_category_path %>

  <ul>
  <% @categories.each do |category| %>
    <li><%= category.name  %> <%= link_to t("administration.categories.index.view.edit"), edit_administration_category_path(category) %></li>
  <% end %>
  </ul>

    <%= will_paginate @categories %>
</column-mc:>