<%= content_for :column_mc do -%>
  <header back="true"><%= t('administration.vat_rates.edit.view.header', :name => @vat_rate.country.name) %></header>

  <%= semantic_form_for @vat_rate, :as => :vat_rate, :url => administration_vat_rate_path(@vat_rate), :input_html => {:method => :put} do |form| %>
      <frm>
        <param:content>
          <%= render :partial => 'form', :locals => {:form => form} %>
        </param:content>
        <param:actions>
          <ibt c="fltri" icon="save">
            <%= form.submit t("administration.vat_rates.edit.view.button_save") %>
          </ibt>
          <ibt c="fltri bt_grey">
            <%= button_to_function t("common.cancel_link"), href(edit_administration_setting_path) %>
          </ibt>
        </param:actions>
      </frm>
  <% end %>
<% end %>