<%= content_for :column_mc do -%>
  <%= header(:back => true) do |section| %>
    <% section.block_header do %>
      <%= t("login_time_requests.new.header") %>
    <% end %>
  <% end %>

  <%= render :partial => 'form', :locals => { :resource => resource, :type => 'new' } %>

  <script>
