<% if current_user.team_buyers? %>
    <%= main_menu_link_to t('layout.main_menu.supplier.subaccounts'), suppliers_subaccounts_path, :tab => 'subaccounts' %>
    <%= main_menu_link_to t('layout.main_menu.supplier.lead_requests'), suppliers_lead_requests_path, :tab => 'requested_leads' %>
<% end %>
<%= main_menu_link_to t('layout.main_menu.supplier.invoices'), suppliers_invoices_path, :tab => 'invoices' %>
<% if current_user.pipeline_reports_enabled? %>
    <%= main_menu_link_with_subtabs_to :title => t('layout.main_menu.supplier.pipeline_reports'), :tab => 'pipeline_reports' do %>
        <% %w(state week month).each do |order_name| %>
            <%= sub_menu_link_to content_tag(:span, t("layout.main_menu.supplier.pipeline_reports_by_#{order_name}")),
                                 suppliers_pipeline_reports_path(:order => order_name),
                                 :subtab => "pipeline_reports_#{order_name}" %>
        <% end %>
    <% end %>
<% end %>
